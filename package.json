{
  "name": "api",
  "version": "1.0.0",
  "description": "resto api",
  "main": "index.js",
  "scripts": {
    "generate": "prisma generate",
    "seeds:recreate": "ts-node ./prisma/seeds/generate.ts",
    "seeds": "prisma migrate reset",
    "prisma:deploy": "prisma migrate dev --create-only && prisma migrate deploy && prisma generate",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only --exit-child --watch src src/index",
    "prod": "npm run build && node build/src/index",
    "build": "prisma generate && tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css build/src",
    "start": "node build/src/index",
    "test": "jest",
    "reserves": "NODE_ENV=development ts-node-dev --watch prisma prisma/scripts/addReserves.ts"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.0",
    "@types/morgan": "^1.9.5",
    "@types/multiparty": "^0.0.33",
    "@types/node": "^18.7.14",
    "@types/node-telegram-bot-api": "^0.61.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/request-ip": "^0.0.38",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.1",
    "babel-jest": "^29.2.2",
    "jest": "^29.2.2",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.338.0",
    "@prisma/client": "^4.14.0",
    "@trpc/server": "^10.29.1",
    "algoliasearch": "^4.19.1",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.11.0",
    "copyfiles": "^2.4.1",
